[loggers]
keys=root

[logger_root]
handlers=file
level=NOTSET

[formatters]
keys=complex

[formatter_complex]
class=colorlog.ColoredFormatter
format=[%(log_color)s%(levelname)-8s%(reset)s][%(asctime)s][%(blue)s%(name)s%(reset)s]-%(message)s

[handlers]
keys=file

[handler_file]
class=handlers.RotatingFileHandler
formatter=complex
level=WARNING
args=('/var/www/sonicAuto/logs/sonic-platform.log','a',1000000,10)
